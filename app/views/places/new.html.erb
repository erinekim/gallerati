<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1>Submit Art You've Seen or Created</h1>
		<%= simple_form_for @place do |f| %>
			<%= f.input :name %>
			<%= f.input :address %>
			<%= f.input :description %> 
			<%= f.input :photos %> <!-- added during self-guided study -->
			<!-- f.input caption %> -->
			<br />
			<%= f.submit 'Create', :class => 'btn btn-primary' %>
		<% end %>
</div>